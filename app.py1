        "# 4. Model Evaluation\n",
        "# Updated evaluate_model function with error handling\n",
        "def evaluate_model(pipeline, X_test, y_test, model_name):\n",
        "    y_pred = pipeline.predict(X_test)\n",
        "    print(f\"\\n{model_name} Evaluation:\")\n",
        "    print(f\"Accuracy: {accuracy_score(y_test, y_pred):.4f}\")\n",
        "    print(f\"Precision: {precision_score(y_test, y_pred, zero_division=0):.4f}\")\n",
        "    print(f\"Recall: {recall_score(y_test, y_pred, zero_division=0):.4f}\")\n",
        "    print(f\"F1-Score: {f1_score(y_test, y_pred, zero_division=0):.4f}\")\n",
        "\n",
        "    # Check if predict_proba returns probabilities for both classes\n",
        "    try:\n",
        "        y_proba = pipeline.predict_proba(X_test)\n",
        "        print(f\"predict_proba shape: {y_proba.shape}\")\n",
        "        if y_proba.shape[1] == 2:\n",
        "            print(f\"ROC-AUC: {roc_auc_score(y_test, y_proba[:, 1]):.4f}\")\n",
        "        else:\n",
        "            print(f\"ROC-AUC: Not available (only {y_proba.shape[1]} class(es) predicted)\")\n",
        "    except AttributeError:\n",
        "        print(\"ROC-AUC: Not available (model does not support predict_proba)\")\n",
        "    except IndexError:\n",
        "        print(\"ROC-AUC: Not available (index error in predict_proba)\")"
      ],
