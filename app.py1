        "# Standardize categorical columns\n",
        "df['Gender'] = df['Gender'].apply(lambda x: x if x in ['Male', 'Female', 'Other'] else 'Other')\n",
        "df['Admission_Type'] = df['Admission_Type'].apply(lambda x: x if x in ['Emergency', 'Urgent', 'Elective'] else 'Unknown')\n",
        "df['Diagnosis'] = df['Diagnosis'].apply(lambda x: x if x in ['Heart Disease', 'Diabetes', 'Injury', 'Infection'] else 'Unknown')\n",
        "df['Readmitted'] = df['Readmitted'].apply(lambda x: 1 if x == 'Yes' else 0)  # Convert to binary"
